{
  "name": "@bkrmadtya/safe-env",
  "version": "1.0.3",
  "description": "A thin wrapper around dotenv to make it easy, safe and verbose to access environment variables.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bkrmadtya/safe-env"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsup ./src",
    "test": "vitest --config ./vite.config.ts"
  },
  "keywords": [
    "dotenv",
    "safe",
    "safe-env",
    "production",
    "typescript"
  ],
  "author": "@bkrmadtya 2023",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "publishConfig": {
    "tag": "latest",
    "registry": "https://npm.pkg.github.com/bkrmadtya"
  }
}
